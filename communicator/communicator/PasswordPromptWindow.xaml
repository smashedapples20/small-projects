<Window x:Name="Security" x:Class="Unlimitedinf.OneOff.Communicator.PasswordPromptWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Unlimitedinf.OneOff.Communicator"
        mc:Ignorable="d"
        Title="Password Entry" Height="80" Width="200" Icon="unlimitedinf.ico" ResizeMode="NoResize" WindowStyle="ToolWindow" ShowInTaskbar="False">
    <Grid>
        <PasswordBox x:Name="passbox" Margin="10,10,10,0" VerticalAlignment="Top" KeyUp="box_keyup"/>
        <TextBox x:Name="clearpassbox" Height="23" Margin="10,10,10,0" VerticalAlignment="Top" KeyUp="box_keyup" Visibility="Hidden"/>
    </Grid>
</Window>
